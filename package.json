{
   "name": "@faulkj/toaster",
   "version": "2.0.1",
   "description": "A highly customizable vanilla JavaScript library used to cleanly serve toast notifications.",
   "type": "module",
   "main": "dist/js/toaster.js",
   "typings": "src/types/toaster.d.ts",
   "scripts": {
      "build": "del dist && tsc && rollup -c --configPlugin typescript && del dist/ts",
      "minify": "rollup -c --environment NODE_ENV:production --configPlugin typescript && del dist/ts"
   },
   "repository": {
      "type": "git",
      "url": "git+https://github.com/faulkj/toaster.git"
   },
   "keywords": [
      "toast",
      "notifications",
      "javascript",
      "vanilla javascript"
   ],
   "author": "Joshua Faulkenberry <j@joshuafaulkenberry.com> (https://joshuafaulkenberry.com/)",
   "license": "Kopimi",
   "bugs": {
      "url": "https://github.com/faulkj/toaster/issues"
   },
   "homepage": "https://github.com/faulkj/toaster#readme",
   "devDependencies": {
      "@rollup/plugin-terser": "^0.4.1",
      "@rollup/plugin-typescript": "^11.1.0",
      "del": "^7.0.0",
      "rollup": "^3.21.4",
      "rollup-plugin-postcss": "^4.0.2",
      "sass": "^1.62.1",
      "tslib": "^2.5.0",
      "typescript": "^5.0.4"
   }
}
